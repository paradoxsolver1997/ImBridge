[
    {
        "key": "ghostscript",
        "display": "Ghostscript",
        "executables": ["gswin64c", "gswin32c", "gs"],
        "config_key": "ghostscript_path",
        "type": "exe",
        "desc": "Interpreter for PostScript and PDF files, used for rendering, conversion, and processing.",
        "homepage": "https://www.ghostscript.com/"
    },
    {
        "key": "pstoedit",
        "display": "pstoedit",
        "executables": ["pstoedit"],
        "config_key": "pstoedit_path",
        "type": "exe",
        "desc": "Converts PostScript and PDF graphics to other vector formats such as SVG, DXF, EMF, etc.",
        "homepage": "http://www.pstoedit.net/"
    },
    {
        "key": "cairosvg",
        "display": "cairosvg",
        "executables": ["cairosvg"],
        "config_key": null,
        "type": "python",
        "desc": "SVG converter based on Cairo, supports SVG to PNG/PDF/PS/EPS conversion.",
        "homepage": "https://cairosvg.org/"
    },
    {
        "key": "scour",
        "display": "Scour SVG Optimizer",
        "executables": ["scour"],
        "config_key": null,
        "type": "python",
        "desc": "SVG optimizer that cleans and compresses SVG files, improves browser compatibility and reduces file size.",
        "homepage": "https://github.com/scour-project/scour"
    },
    {
        "key": "potrace",
        "display": "Potrace",
        "executables": ["potrace"],
        "config_key": "potrace_path",
        "type": "exe",
        "desc": "Bitmap tracing tool, converts bitmaps (BMP) to smooth, scalable images (EPS, SVG, PDF, PS).",
        "homepage": "http://potrace.sourceforge.net/"
    },
    {
        "key": "libcairo-2.dll",
        "display": "cairosvg library",
        "executables": ["libcairo-2.dll"],
        "config_key": null,
        "type": "dll",
        "desc": "Cairo graphics library DLL, required for cairosvg and related rendering.",
        "homepage": "https://www.cairographics.org/"
    },
    {
        "key": "pymupdf",
        "display": "PyMuPDF",
        "executables": ["fitz"],
        "config_key": null,
        "type": "python",
        "desc": "Python binding for MuPDF, supports reading, analyzing, and editing PDF and XPS files, including vector and raster content.",
        "homepage": "https://pymupdf.readthedocs.io/"
    },
    {
        "key": "pillow",
        "display": "Pillow",
        "executables": ["PIL"],
        "config_key": null,
        "type": "python",
        "desc": "Python Imaging Library (PIL) fork, supports opening, manipulating, and saving many image file formats.",
        "homepage": "https://python-pillow.org/"
    },
    {
        "key": "pillow-heif",
        "display": "pillow-heif",
        "executables": ["pillow_heif"],
        "config_key": null,
        "type": "python",
        "desc": "HEIF/HEIC image format plugin for Pillow, enables reading and writing HEIF/HEIC images in Pillow.",
        "homepage": "https://github.com/romanmoravcik/pillow-heif"
    },
    {
        "key": "pdftops",
        "display": "pdftops",
        "executables": ["pdftops"],
        "config_key": "pdftops_path",
        "type": "exe",
        "desc": "Converts PDF files to PostScript (PS) or Encapsulated PostScript (EPS) format. Part of the Xpdf or Poppler utilities.",
        "homepage": "https://www.xpdfreader.com/pdftops-man.html"
    }
]
